  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "check-code-style": "eslint --color ./",
    "fix-code-style": "eslint --fix --color ./",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -s ./public -p 6006",
    "fix-code-style-diff": "lint-staged",
    "build-storybook": "build-storybook --output-dir storybook-dist",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron electron/main\"",
    "prepackage": "PUBLIC_URL=./ npm run build",
    "package": "electron-builder",
    "package:all": "electron-builder -mwl"
